<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="15dp"
    android:layout_marginTop="13dp"
    android:layout_marginEnd="15dp"
    android:layout_marginBottom="15dp">

    <ImageView
        android:id="@+id/imgv_home_choose_main"
        android:layout_width="240dp"
        android:layout_height="140dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_choose_one_o" />

    <ImageView
        android:id="@+id/imgv_home_choose_up"
        android:layout_width="65dp"
        android:layout_height="70dp"
        android:layout_marginStart="5dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toEndOf="@+id/imgv_home_choose_main"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_choose_one_t" />

    <ImageView
        android:id="@+id/imgv_home_choose_down"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toEndOf="@+id/imgv_home_choose_main"
        app:layout_constraintTop_toBottomOf="@+id/imgv_home_choose_up"
        app:srcCompat="@drawable/img_choose_one_th" />

    <TextView
        android:id="@+id/tv_home_choose_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="롯데리아 영등포역사점"
        android:textColor="@color/black"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgv_home_choose_main" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="17dp"
        android:layout_height="17dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/ic_review_star"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_home_choose_name" />

    <TextView
        android:id="@+id/tv_home_choose_review"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:layout_marginTop="10dp"
        android:text="3.0(1)"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/tv_home_choose_name"
        tools:layout_editor_absoluteY="187dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="13dp"
        android:src="@drawable/ic_middle_spot"
        app:layout_constraintStart_toEndOf="@id/tv_home_choose_review"
        app:layout_constraintTop_toBottomOf="@+id/tv_home_choose_name" />

    <TextView
        android:id="@+id/tv_home_choose_distance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        android:text="2.9"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/imageView9"
        app:layout_constraintTop_toBottomOf="@+id/tv_home_choose_name" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="km"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/tv_home_choose_distance"
        app:layout_constraintTop_toBottomOf="@+id/tv_home_choose_name" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="13dp"
        android:src="@drawable/ic_middle_spot"
        app:layout_constraintStart_toEndOf="@id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/tv_home_choose_name" />

    <TextView
        android:id="@+id/tv_home_chosse_delivery_fee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        android:text="배달비 2000원"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/imageView10"
        app:layout_constraintTop_toBottomOf="@+id/tv_home_choose_name" />

    <TextView
        android:id="@+id/tv_home_choose_delivery_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="25~35분"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgv_home_choose_down" />
</androidx.constraintlayout.widget.ConstraintLayout>